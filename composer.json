{
    "name": "laravel/laravel",
    "type": "project",
    "description": "A prototype of Voyager with Laravel.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "artesaos/seotools": "^0.16.0",
        "dingo/api": "^2.4",
        "fideloper/proxy": "^4.0",
        "hmazter/laravel-schedule-list": "^2.0",
        "laravel/framework": "^6.0",
        "laravel/helpers": "^1.1",
        "laravel/passport": "^7.3",
        "laravel/telescope": "^2.0",
        "laravel/tinker": "^1.0",
        "nwidart/laravel-modules": "^6.0",
        "renatomarinho/laravel-page-speed": "^1.8",
        "spatie/laravel-activitylog": "^3.8",
        "symfony/console": "4.3.4",
        "tcg/voyager": "1.3.x-dev",
        "tymon/jwt-auth": "^1.0",
        "unisharp/laravel-filemanager": "^1.9"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "facade/ignition": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "martinlindhe/laravel-vue-i18n-generator": "^0.1.42",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": {
        "hooks": {
            "type": "composer",
            "url": "https://larapack.io"
        }
    }
}
